<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Test Polygons</name>
    <!-- Rectangle A: center ~(10, 10), spans 9.5-10.5 lat, 9.5-10.5 lng -->
    <Placemark>
      <name>Rectangle A</name>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              9.5,9.5,0
              10.5,9.5,0
              10.5,10.5,0
              9.5,10.5,0
              9.5,9.5,0
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
    <!-- Triangle B: overlaps Rectangle A's east edge for merge tests -->
    <Placemark>
      <name>Triangle B</name>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              10.3,9.5,0
              12.3,10.0,0
              10.3,10.5,0
              10.3,9.5,0
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
    <!-- Complex C: 23 vertices for simplification tests -->
    <Placemark>
      <name>Complex C</name>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
